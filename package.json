{
  "name": "react-badger",
  "version": "2.0.3",
  "description": "Simple React notification badge component",
  "main": "lib/index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "npm run build:transpile",
    "build:transpile": "babel src --out-dir lib",
    "clean": "rimraf lib/** dist/** docs/ghpages *.tgz examples/**/bundle.js",
    "dist": "npm run dist:dev && npm run dist:prod",
    "dist:dev": "webpack --config webpack.build.js -d",
    "dist:prod": "NODE_ENV=production webpack --config webpack.build.js -p",
    "gh-pages": "webpack -p && cpx 'examples/basic/*' docs/ghpages && rimraf docs/ghpages/*.jsx && gh-pages -d docs/ghpages && rimraf node_modules/gh-pages/.cache",
    "lint": "standard",
    "prepublishOnly": "npm run clean && npm run build",
    "start": "webpack-dev-server",
    "test": "exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntamas/react-badger.git"
  },
  "author": "Tamas Nepusz <ntamas@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ntamas/react-badger/issues"
  },
  "homepage": "https://github.com/ntamas/react-badger#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "cpx": "^1.5.0",
    "gh-pages": "^3.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "standard": "^14.3.4",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "peerDependencies": {
    "react": "^16.7.0"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react-transition-group": "^4.4.1"
  }
}
